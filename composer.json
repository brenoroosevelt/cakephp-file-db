{
    "name": "brenoroosevelt/cakephp-file-db",
    "description": "Armazena arquivos no Banco - plugin para CakePHP 3.x",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.1 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FileDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FileDb\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
